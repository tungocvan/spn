	<script>
	  $(document).ready(function() {
	        
			var availableSchool;
				$.ajax({
					type: "POST",
	                url: "https://duhocbluesea.edu.vn/tim-kiem/getallschool",				
					data: {
						'post_ids': '0'
					}
	            }).done(function(data){
	                availableSchool = data.split(',');
					$( "#disabled_input" ).autocomplete({
						source: availableSchool          
					});
	            });
				
	    });
		
	</script>


